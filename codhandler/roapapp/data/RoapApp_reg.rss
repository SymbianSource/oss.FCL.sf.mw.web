#include <appinfo.rh>
#include <RoapApp.rsg>
#include <data_caging_paths_strings.hrh>

UID2 KUidAppRegistrationResourceFile
UID3 0x10008d64

RESOURCE APP_REGISTRATION_INFO
    {
    app_file = "RoapApp";
    localisable_resource_file = APP_RESOURCE_DIR"\\RoapApp";
    localisable_resource_id = R_ROAP_APP_LOCALISABLE_APP_INFO;
    hidden = KAppIsHidden;
    embeddability = KAppEmbeddable;
    newfile = KAppDoesNotSupportNewFile;
    datatype_list =
        {
        DATATYPE
            {
            priority = EDataTypePriorityNormal;
            type = "application/vnd.oma.drm.roap-trigger+xml";
            },
        DATATYPE
            {
            priority = EDataTypePriorityNormal;
            type = "application/vnd.oma.drm.roap-pdu+xml";
            }
        };
    }
